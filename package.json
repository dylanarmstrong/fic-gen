{
  "name": "@dylanarmstrong/fic-gen",
  "author": "Dylan Armstrong <dylan@dylan.is>",
  "description": "ePub output for online web fiction",
  "homepage": "https://github.com/dylanarmstrong/fic-gen",
  "bugs": {
    "url": "https://github.com/dylanarmstrong/fic-gen/issues"
  },
  "main": "./lib/index.js",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanarmstrong/fic-gen"
  },
  "type": "module",
  "version": "0.0.1",
  "bin": {
    "fic-gen": "./lib/index.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "prettier --write --ignore-unknown src/* && eslint src/"
  },
  "engines": {
    "node": ">16.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@dylanarmstrong",
      "prettier"
    ],
    "rules": {
      "dot-notation": "off"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@dylanarmstrong/eslint-config": "^0.3.0",
    "@types/argparse": "^2.0.10",
    "@types/defaults": "^1.0.3",
    "@types/md5": "^2.3.2",
    "@types/node": "^16.18.34",
    "@types/nodepub": "^3.0.0",
    "@types/sanitize-html": "^2.9.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "cheerio": "1.0.0-rc.12",
    "defaults": "^1.0.4",
    "htmlparser2": "^8.0.2",
    "md5": "^2.3.0",
    "nodepub": "^3.0.8",
    "parse5": "^7.1.2",
    "sanitize-html": "^2.10.0",
    "uuid": "^9.0.0"
  }
